  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/gif.git"
  },
  "publishConfig": {
