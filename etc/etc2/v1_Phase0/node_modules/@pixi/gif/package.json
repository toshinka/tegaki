  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "gif",
    "animation",
    "pixi",
    "pixijs"
  ],
  "author": "Matt Karl @bigtimebuddy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pixijs/gif/issues"
  },
  "homepage": "https://github.com/pixijs/gif#readme",
  "pre-commit": [
    "lint",
    "types"
  ],
  "peerDependencies": {
    "@pixi/assets": "^7.0.0",
    "@pixi/core": "^7.0.0",
    "@pixi/display": "^7.0.0",
    "@pixi/sprite": "^7.0.0"
  }
}
